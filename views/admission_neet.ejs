<script>
     $(document).ready(()=>
     {
      
         $('#admission_form').submit(()=>
         {

               event.preventDefault();
                            
               if($('#email_checkbox').is(':checked'))
               {
                  const form_data = $('#admission_form').serialize();
                  $.ajax({
                      type: 'POST',
                      url: '/submit_form',
                      data: form_data,
                      success: function (response) {
                          $('#admission_form')[0].reset();
                          Swal.fire(response)
                      },
                      error: function (error) {
                          Swal.fire(error.responseJSON)
                      }
                  });
               }
               else
               {
                Swal.fire('Please Tick the checkbox')
               }
         })
     })

     $(document).ready(function(){
    $('.rankers-carousel').owlCarousel({
       margin: 20,
       loop: true,
       autoplay: true,
       autoplayTimeOut: 100,
       autoplayHoverPause: true,
       nav: false,
       responsive: {
           0:{
               items: 1,
           },
           600:{
               items: 1,
           },
           1000:{
               items: 1,
           }
       }
   });

   
})
</script>
